{% extends 'base.html.twig' %}

{% block body %}
    <h1>Proprio</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ proprio.id }}</td>
            </tr>
            <tr>
                <th>Prenom</th>
                <td>{{ proprio.prenom }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ proprio.nom }}</td>
            </tr>
            <tr>
                <th>Tel</th>
                <td>{{ proprio.tel }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('admin_proprio_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('admin_proprio_edit', { 'id': proprio.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
